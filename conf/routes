GET         /ml/regression/all                                                      controllers.ml.RegressionController.listAll(s: String ?= "concreteClass")
GET         /ml/regression/new                                                      controllers.ml.RegressionController.create(concreteClassName: String)
POST        /ml/regression/save                                                     controllers.ml.RegressionController.save
GET         /ml/regression/get/:id                                                  controllers.ml.RegressionController.get(id: BSONObjectID)
POST        /ml/regression/:id                                                      controllers.ml.RegressionController.update(id: BSONObjectID)
POST        /ml/regression/:id/delete                                               controllers.ml.RegressionController.delete(id: BSONObjectID)
GET         /ml/regression/idAndNames                                               controllers.ml.RegressionController.idAndNames

